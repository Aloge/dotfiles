# Load things
[[ -f "$HOME/shell/.aliases" ]] && source "$HOME/.aliases"
[[ -f "$HOME/.functions" ]] && source "$HOME/.functions"

# Adjust $PATH
[[ -d "$HOME/.dotfiles/scripts" ]] && export PATH=$HOME/.scripts:$PATH
[[ -d "$HOME/.dotfiles/bin" ]] && export PATH=$HOME/.lein/bin:$PATH

# Locale
export LC_ALL=en_US.UTF-8  
export LANG=en_US.UTF-8


export EDITOR=subl


